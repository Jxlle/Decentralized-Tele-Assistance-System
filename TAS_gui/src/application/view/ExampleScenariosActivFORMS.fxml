<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="272.0" prefWidth="1014.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.view.controller.ExampleScenariosActivFORMS">
   <children>
      <Label layoutX="75.0" layoutY="65.0" text="Select a scenario for TAS application to run:" />
      <ToggleButton fx:id="btnWithoutAdaptation" layoutX="75.0" layoutY="108.0" mnemonicParsing="false" prefHeight="89.0" prefWidth="261.0" selected="true" text="Without Adaptation">
         <toggleGroup>
            <ToggleGroup fx:id="toggleGroupScenarios" />
         </toggleGroup>
      </ToggleButton>
      <ToggleButton fx:id="btnRetry" layoutX="377.0" layoutY="108.0" mnemonicParsing="false" prefHeight="89.0" prefWidth="261.0" text="With Retry Service Invocations" toggleGroup="$toggleGroupScenarios" />
      <ToggleButton fx:id="btnWithAdaptation" layoutX="670.0" layoutY="108.0" mnemonicParsing="false" prefHeight="89.0" prefWidth="261.0" text="With Adaptation" toggleGroup="$toggleGroupScenarios" />
   </children>
</AnchorPane>
