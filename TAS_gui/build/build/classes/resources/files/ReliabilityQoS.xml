<?xml version="1.0" ?>
<inputProfile>
	<maxSteps>50</maxSteps>
	<qosRequirement>ReliabilityQoS</qosRequirement>
	<variables>
		<variable>
			<name>patientId</name>
			<values>
				<value>
					<data class="int">1</data>
					<ratio>1.0</ratio>
				</value>
			</values>
		</variable>
		<variable>
			<name>pick</name>
			<values>
				<value>
				<!-- Invoke Medical Analysis Service with setting value of pick 1 -->
					<data class="int">1</data>
					<ratio>0.75</ratio>
				</value>
				<value>
				<!-- Invoke AlarmService with setting value of pick 2 -->
					<data class="int">2</data>
					<ratio>0.25</ratio>
				</value>
			</values>
		</variable>
	</variables>
</inputProfile>